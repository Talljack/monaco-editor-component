{
  "name": "monaco-editor-component",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.3",
  "description": "Monaco Editor and DiffEditor, support Vue and React.",
  "author": "Talljack",
  "license": "MIT",
  "homepage": "https://github.com/Talljack/monaco-editor-component#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Talljack/monaco-editor-component.git"
  },
  "bugs": {
    "url": "https://github.com/Talljack/monaco-editor-component/issues"
  },
  "keywords": [
    "typescript",
    "react",
    "monaco-editor",
    "tailwindcss",
    "monaco-editor-component",
    "monaco-diff-editor",
    "vue",
    "monaco-editor-vue",
    "monaco-editor-react"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/type.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "react": [
        "./dist/react/index.d.ts"
      ],
      "vue": [
        "./dist/vue/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:react": "pnpm -C demoPkg/react dev",
    "dev:vue": "pnpm -C demoPkg/vue dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --cache --write .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --incremental --noEmit && rimraf tsconfig.tsbuildinfo",
    "clean": "rimraf dist node_modules"
  },
  "dependencies": {
    "monaco-editor": "^0.52.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.1",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "talljack-eslint-config": "^0.2.6",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
